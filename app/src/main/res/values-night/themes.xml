<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Partner_organikita" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#00aa13</item>
        <item name="colorPrimaryVariant">#00aa13</item>
        <item name="colorOnPrimary">#00aa13</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#00aa13</item>
        <item name="colorSecondaryVariant">#00aa13</item>
        <item name="colorOnSecondary">#00aa13</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="CustomTabStyle">
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">#858585</item>
        <item name="android:textSize">12dp</item>
        <item name="android:fontFamily">@font/poppinsbold</item>
    </style>


    <style name="BottomSheetDialogTheme" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/BottomSheetStyle</item>
    </style>

    <style name="BottomSheetStyle" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@android:color/transparent</item>
    </style>

</resources>