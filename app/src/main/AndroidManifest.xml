<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.partner_organikita">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/organikitablack"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/config_network"
        android:roundIcon="@drawable/organikitablack"
        android:supportsRtl="true"
        android:theme="@style/Theme.Partner_organikita"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".activity.LoginActivity"/>
        <activity android:name=".activity.AddProductActivity" />
        <activity android:name=".activity.DetailTransaksiActivity" />
        <activity android:name=".activity.ChooseDelivery" />
        <activity android:name=".activity.SuccessActivity" />
        <activity android:name=".activity.KebijakanPrivasiActivity" />
        <activity android:name=".activity.KetentuanLayananActivity" />
        <activity android:name=".activity.HubungiKamiActivity" />
        <activity android:name=".activity.AddImageActivity" />
    </application>

</manifest>